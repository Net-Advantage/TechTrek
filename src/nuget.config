<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear />
		<add key="Nabs GitHub Packages" value="https://nuget.pkg.github.com/Net-Advantage/index.json" protocolVersion="3" />
		<add key="Net8 Daily Builds" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet8/nuget/v3/index.json" protocolVersion="3" />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
	</packageSources>
	<solution>
		<add key="disableSourceControlIntegration" value="true" />
	</solution>
	<packageSourceMapping>
		<clear />
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<packageSource key="Net8 Daily Builds">
			<package pattern="Aspire*" />
			<package pattern="Microsoft.Extensions.ServiceDiscovery*" />
			<!-- Add more patterns as needed -->
		</packageSource>
		<packageSource key="Nabs GitHub Packages">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
</configuration>