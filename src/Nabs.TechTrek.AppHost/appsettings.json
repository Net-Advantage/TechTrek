{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning",
			"Aspire.Hosting.Dcp": "Warning"
		}
	},
	"Aspire": {
		"Azure": {
			"Security": {
				"KeyVault": {
					"TechTrekKeyVault": {
						"VaultUri": "YOUR_VAULT_URI",
						"HealthChecks": true,
						"Tracing": false,
						"ClientOptions": {
							"DisableChallengeResourceVerification": true
						}
					}
				}
			}
		},
		"Microsoft": {
			"EntityFrameworkCore": {
				"TechTrekDbContext": {
					"ConnectionString": "Server=.;Initial Catalog=TechTrekSqlDb;Persist Security Info=False;Trusted_Connection=True;Connection Timeout=30;Trust Server Certificate=true;",
					"DbContextPooling": true,
					"HealthChecks": true,
					"Tracing": false,
					"Metrics": true
				}
			}
		}
	}
}
